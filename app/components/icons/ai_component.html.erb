<svg xmlns="http://www.w3.org/2000/svg" class="<%= css_class %>" fill="none" viewBox="0 0 100 100">
  <%# Robot head outline %>
  <rect x="15" y="20" width="70" height="60" rx="8" ry="8" fill="currentColor" fill-opacity="0.1" stroke="currentColor" stroke-width="2" />
  <%# Antenna %>
  <line x1="50" y1="20" x2="50" y2="10" stroke="currentColor" stroke-width="2" />
  <circle cx="50" cy="8" r="3" fill="currentColor" />
  <%# Eyes %>
  <circle cx="35" cy="40" r="8" fill="currentColor" />
  <circle cx="65" cy="40" r="8" fill="currentColor" />
  <circle cx="35" cy="40" r="4" fill="currentColor" fill-opacity="0.2" />
  <circle cx="65" cy="40" r="4" fill="currentColor" fill-opacity="0.2" />
  <%# Mouth/speaker grille %>
  <rect x="35" y="55" width="30" height="12" rx="6" ry="6" fill="none" stroke="currentColor" stroke-width="2" />
  <line x1="40" y1="58" x2="40" y2="64" stroke="currentColor" stroke-width="1" />
  <line x1="45" y1="58" x2="45" y2="64" stroke="currentColor" stroke-width="1" />
  <line x1="50" y1="58" x2="50" y2="64" stroke="currentColor" stroke-width="1" />
  <line x1="55" y1="58" x2="55" y2="64" stroke="currentColor" stroke-width="1" />
  <line x1="60" y1="58" x2="60" y2="64" stroke="currentColor" stroke-width="1" />
  <%# Side panels/ears %>
  <rect x="10" y="35" width="8" height="15" rx="2" ry="2" fill="currentColor" />
  <rect x="82" y="35" width="8" height="15" rx="2" ry="2" fill="currentColor" />
</svg>
