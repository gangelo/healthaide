<%= form_with(model: food_qualifier, class: "contents") do |form| %>
  <%= render "shared/active_record_errors", resource: food_qualifier %>
  <div class="my-5">
    <%= form.label :qualifier_name, class: "block font-medium text-gray-700 mb-1" %>
    <%= form.text_field :qualifier_name, class: "block rounded-md border-gray-300 shadow-sm focus:border-blue-500 focus:ring-blue-500 w-full", maxlength: 64, required: true %>
    <%= render partial: "shared/maximum_characters", locals: { max_characters: 64, model: FoodQualifier } %>
  </div>
  <%= render partial: "shared/forms/form_buttons", locals: {
    form: form,
    resource: food_qualifier,
    back_path: food_qualifiers_path
  } %>
<% end %>
